{
    "name": "ambrosus-javascript-sdk",
    "version": "2.0.4",
    "description": "Client SDK to simplify connecting to the Ambrosus API",
    "main": "lib/ambrosus.cjs.min.js",
    "module": "lib/ambrosus.umd.min.js",
    "directories": {
        "example": "examples",
        "test": "test"
    },
    "scripts": {
        "build": "rollup --config rollup.config.prod.js && npm run build-js && npm run copy",
        "build:dev": "rollup --config rollup.config.dev.js",
        "build-js": "node makeBrowserify.js",
        "copy": "node copyFiles.js",
        "release-patch": "npm version patch -m 'chore(*): patch version'",
        "release-minor": "npm version minor -m 'chore(*): minor version'",
        "release-major": "npm version major -m 'chore(*): major version'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ambrosus/sdk-javascript2.git"
    },
    "author": "Ambrosus",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ambrosus/sdk-javascript2/issues"
    },
    "homepage": "https://github.com/ambrosus/sdk-javascript2#readme",
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "@types/codemirror": "0.0.72",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.0.3",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^10.0.0",
        "browserify": "^16.5.1",
        "chai": "^4.1.2",
        "chokidar": "^2.0.3",
        "copy-files": "^0.1.0",
        "eslint": "^5.15.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "jsdoc": "^3.6.4",
        "minami": "^1.2.3",
        "mocha": "^4.0.1",
        "nyc": "^12.0.2",
        "request": "2.87.0",
        "rollup": "^1.4.1",
        "rollup-plugin-babel": "^3.0.4",
        "rollup-plugin-babel-minify": "^5.0.0",
        "rollup-plugin-buble": "^0.19.6",
        "rollup-plugin-delete": "^0.1.2",
        "rollup-plugin-eslint": "^4.0.0",
        "rollup-plugin-filesize": "^2.0.0",
        "rollup-plugin-gzip": "^2.2.0",
        "rollup-plugin-json": "^3.0.0",
        "rollup-plugin-license": "^0.8.1",
        "rollup-plugin-node-resolve": "^4.0.1",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-watch": "^4.3.1",
        "uglifyify": "^5.0.2",
        "xhr2": "^0.1.4",
        "yargs": "^10.0.3"
    },
    "dependencies": {
        "web3": "^1.2.8"
    }
}
