{
  "name": "@inc4/eslint-config-react",
  "version": "1.0.1",
  "description": "ESLint React configuration for JS projects based on airbnb and Prettier configs",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write **/*.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "javascript",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "airbnb-config",
    "prettier-config",
    "react",
    "hooks",
    "react-hooks",
    "inc4"
  ],
  "author": "Alex Molotsylo",
  "license": "MIT",
  "prettier": "@inc4/prettier-config",
  "dependencies": {
    "@inc4/prettier-config": "^1.0.0",
    "eslint": "^6.8.0",
    "prettier": "^2.0.4"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1"
  },
  "peerDependencies": {
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inc4/eslint-config-react.git"
  },
  "bugs": {
    "url": "https://github.com/inc4/eslint-config-react/issues"
  },
  "homepage": "https://github.com/inc4/eslint-config-react#readme"
}
